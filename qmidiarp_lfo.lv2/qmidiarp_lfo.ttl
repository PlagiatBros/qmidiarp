@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix lv2ev: <http://lv2plug.in/ns/ext/event#> .
@prefix lv2ui: <http://lv2plug.in/ns/extensions/ui#> .

<https://git.code.sf.net/p/qmidiarp/code>
    a lv2:Plugin, lv2:InstrumentPlugin ;
    doap:name "qmidiarp_lfo" ;
    doap:maintainer [
        foaf:name "Frank Kober" ;
        foaf:homepage <http://qmidiarp.sourceforge.net> ;
        foaf:mbox <mailto:qmidiarp-devel@lists.sourceforge.net> ;
    ] ;
    lv2:requiredFeature <http://lv2plug.in/ns/ext/urid#map> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2ui:ui <https://git.code.sf.net/p/qmidiarp/code#ui> ;
    lv2:port [
        a lv2:InputPort, lv2ev:EventPort ;
        lv2ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent> ;
        lv2:index 0;
        lv2:symbol "in";
        lv2:name "MIDI In";
    ] ;
    lv2:port [
        a lv2:OutputPort, lv2ev:EventPort ;
        lv2ev:supportsEvent <http://lv2plug.in/ns/ext/midi#MidiEvent> ;
        lv2:index 1;
        lv2:symbol "out";
        lv2:name "MIDI Out";
    ]  ;
    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "AMPLITUDE" ;
        lv2:name "Amplitude" ;
        lv2:default 64.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 128.0 ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "OFFSET" ;
        lv2:name "Offset" ;
        lv2:default 64.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 128.0 ;
    ] ;
    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "RESOLUTION" ;
        lv2:name "Resolution" ;
        lv2:portProperty lv2:integer ;
        lv2:scalePoint [ rdfs:label "1"; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "2"; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "4"; rdf:value 2 ] ;
        lv2:scalePoint [ rdfs:label "8"; rdf:value 3 ] ;
        lv2:scalePoint [ rdfs:label "16"; rdf:value 4 ] ;
        lv2:scalePoint [ rdfs:label "32"; rdf:value 5 ] ;
        lv2:scalePoint [ rdfs:label "64"; rdf:value 6 ] ;
        lv2:scalePoint [ rdfs:label "96"; rdf:value 7 ] ;
        lv2:scalePoint [ rdfs:label "192"; rdf:value 8 ] ;
        lv2:default 4.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 8.0 ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "LENGTH" ;
        lv2:name "Length" ;
        lv2:portProperty lv2:integer ;
        lv2:scalePoint [ rdfs:label "1"; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "2"; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "3"; rdf:value 2 ] ;
        lv2:scalePoint [ rdfs:label "4"; rdf:value 3 ] ;
        lv2:scalePoint [ rdfs:label "5"; rdf:value 4 ] ;
        lv2:scalePoint [ rdfs:label "6"; rdf:value 5 ] ;
        lv2:scalePoint [ rdfs:label "7"; rdf:value 6 ] ;
        lv2:scalePoint [ rdfs:label "8"; rdf:value 7 ] ;
        lv2:scalePoint [ rdfs:label "12"; rdf:value 8 ] ;
        lv2:scalePoint [ rdfs:label "16"; rdf:value 9 ] ;
        lv2:scalePoint [ rdfs:label "24"; rdf:value 10 ] ;
        lv2:scalePoint [ rdfs:label "32"; rdf:value 11 ] ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 11.0 ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "FREQUENCY" ;
        lv2:name "Frequency" ;
        lv2:portProperty lv2:integer ;
        lv2:scalePoint [ rdfs:label "1/32"; rdf:value 0 ] ;
        lv2:scalePoint [ rdfs:label "1/16"; rdf:value 1 ] ;
        lv2:scalePoint [ rdfs:label "1/8"; rdf:value 2 ] ;
        lv2:scalePoint [ rdfs:label "1/4"; rdf:value 3 ] ;
        lv2:scalePoint [ rdfs:label "1/2"; rdf:value 4 ] ;
        lv2:scalePoint [ rdfs:label "3/4"; rdf:value 5 ] ;
        lv2:scalePoint [ rdfs:label "1"; rdf:value 6 ] ;
        lv2:scalePoint [ rdfs:label "2"; rdf:value 7 ] ;
        lv2:scalePoint [ rdfs:label "3"; rdf:value 8 ] ;
        lv2:scalePoint [ rdfs:label "4"; rdf:value 9 ] ;
        lv2:scalePoint [ rdfs:label "5"; rdf:value 10 ] ;
        lv2:scalePoint [ rdfs:label "6"; rdf:value 11 ] ;
        lv2:scalePoint [ rdfs:label "7"; rdf:value 12 ] ;
        lv2:scalePoint [ rdfs:label "8"; rdf:value 13 ] ;
        lv2:default 6.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 13.0 ;
    ] ;
    lv2:port [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "CH_IN" ;
        lv2:name "Input Channel" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 15.0 ;
    ] , [
        a lv2:InputPort, lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "CH_OUT" ;
        lv2:name "Output Channel" ;
        lv2:default 0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 15.0 ;
    ] .
<https://git.code.sf.net/p/qmidiarp/code#ui>
    a lv2ui:Qt4UI ;
    lv2ui:binary <qmidiarp_lfo.so> .


